@model List<SitefinityWebApp.Mvc.Models.RecentModel>

@*<h1>
    @Html.Raw(Model.Message)
</h1>*@


        <div class="row">
            @foreach (SitefinityWebApp.Mvc.Models.RecentModel cate in Model)
            {
                <div class="col-xs-12 col-sm-4 col-md-3">
                    <div class="recent-work-wrap">
                        <img class="img-responsive" src="@cate.Portfolio_Photo_Path" alt="">
                        <div class="overlay">
                            <div class="recent-work-inner">
                                <h3><a href="#">@cate.Title</a> </h3>
                                <p>@cate.Portfolio_Description</p>
                                <a class="preview" href="@cate.Portfolio_Photo_Path" rel="prettyPhoto"><i class="fa fa-eye"></i> View</a>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div><!--/.row-->